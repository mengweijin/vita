{
  "name": "vitality-layui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm copy && vite",
    "build": "pnpm copy && vite build",
    "preview": "vite build && vite preview",
    "copy": "xcopy .\\node_modules\\layui\\dist .\\public\\layui /E /H /Y /I",
    "clean": "pnpm clean:windows",
    "clean:dist": "rmdir /s /q dist",
    "clean:windows": "del /q pnpm-lock.yaml && rmdir /s /q node_modules && pnpm store prune && pnpm install",
    "clean:linux": "rm -rf pnpm-lock.yaml && rm -rf node_modules && pnpm store prune && pnpm install"
  },
  "dependencies": {
    
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^6.0.1",
    "glob": "^11.0.1",
    "layui": "2.10.0",
    "vite": "^6.1.1"
  },
  "packageManager": "pnpm@9.8.0+sha1.33f415ae4331f9b29a92dd9bbef959b916d65535"
}
