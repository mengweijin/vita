<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.mengweijin.system.mapper.DictDataMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.mengweijin.system.domain.entity.DictData">
        <id column="ID" property="id" />
        <result column="TYPE_CODE" property="typeCode" />
        <result column="DATA_CODE" property="dataCode" />
        <result column="LABEL" property="label" />
        <result column="SEQ" property="seq" />
        <result column="DISABLED" property="disabled" />
        <result column="REMARK" property="remark" />
        <result column="CREATE_BY" property="createBy" />
        <result column="CREATE_TIME" property="createTime" />
        <result column="UPDATE_BY" property="updateBy" />
        <result column="UPDATE_TIME" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="BaseColumnList">
        TYPE_CODE,
        DATA_CODE,
        LABEL,
        SEQ,
        DISABLED,
        REMARK,
        ID,
        CREATE_BY,
        CREATE_TIME,
        UPDATE_BY,
        UPDATE_TIME
    </sql>

    <!-- 通用查询条件 -->
    <sql id="WhereCondition">
        <where>
            <if test="typeCode != null and typeCode != ''">
                AND TYPE_CODE = #{typeCode}
            </if>
            <if test="dataCode != null and dataCode != ''">
                AND DATA_CODE = #{dataCode}
            </if>
            <if test="label != null and label != ''">
                AND LABEL = #{label}
            </if>
            <if test="seq != null">
                AND SEQ = #{seq}
            </if>
            <if test="disabled != null and disabled != ''">
                AND DISABLED = #{disabled}
            </if>
            <if test="remark != null and remark != ''">
                AND REMARK = #{remark}
            </if>
            <if test="id != null">
                AND ID = #{id}
            </if>
            <if test="createBy != null">
                AND CREATE_BY = #{createBy}
            </if>
            <if test="createTime != null">
                AND CREATE_TIME = #{createTime}
            </if>
            <if test="updateBy != null">
                AND UPDATE_BY = #{updateBy}
            </if>
            <if test="updateTime != null">
                AND UPDATE_TIME = #{updateTime}
            </if>
        </where>
    </sql>

</mapper>
